{
  "name": "react-mock-component",
  "version": "3.1.0",
  "description": "Create type safe mock React components to use in tests",
  "keywords": [
    "tdd",
    "react",
    "spy",
    "stub",
    "mock",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NiGhTTraX/react-mock-component.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/react-mock-component",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run build",
    "release": "standard-version",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "jest --coverage",
    "report-coverage": "codecov"
  },
  "dependencies": {
    "sinon": "~14.0.0"
  },
  "peerDependencies": {
    "@types/react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@nighttrax/eslint-config-tsx": "~10.0.0-beta.2",
    "@tdd-buffet/jest-config": "~5.0.0",
    "@tdd-buffet/tsconfig": "~1.0.4",
    "@testing-library/react": "~13.4.0",
    "@types/jest": "~28.1.6",
    "@types/node": "~16.11.0",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@types/sinon": "~10.0.0",
    "codecov": "~3.8.0",
    "eslint": "~8.25.0",
    "jest": "~28.1.3",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "standard-version": "~9.5.0",
    "typescript": "~4.8.0"
  }
}
